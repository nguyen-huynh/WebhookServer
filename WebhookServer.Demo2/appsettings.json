{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "File": {
      "Path": "Logs/log_{0}.log",
      "Append": true,
      "MinLevel": "Debug",
      "FileSizeLimitBytes": 0, // use to activate rolling file behaviour
      "MaxRollingFiles": 0 // use to specify max number of log files
    }
  },
  "AppConfiguration": {
    "TargetFPS": 30,
    "NoOfWebhooks": 10,
    "InitQueuePerWebhook": 100,
    "WebhookRequestTimeout":  30
  }
}